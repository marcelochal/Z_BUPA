 <asx:abap xmlns:asx="http://www.sap.com/abapxml" version="1.0">
  <asx:values>
   <TOOL>HOOK_IMPL</TOOL>
   <SHORTTEXT>Validação de CNPJ/CPF de fornecedores TAXNUMBER_CHECK_DUPL_VENDOR</SHORTTEXT>
   <ORIGINAL_OBJECT>
    <PGMID>R3TR</PGMID>
    <ORG_OBJ_TYPE>FUGR</ORG_OBJ_TYPE>
    <ORG_OBJ_NAME>ID_TAXNUM</ORG_OBJ_NAME>
    <ORG_MAIN_TYPE>FUGR</ORG_MAIN_TYPE>
    <ORG_MAIN_NAME>ID_TAXNUM</ORG_MAIN_NAME>
    <PROGRAMNAME>SAPLID_TAXNUM</PROGRAMNAME>
   </ORIGINAL_OBJECT>
   <ENHANCEMENTS>
    <ENH_HOOK_IMPL>
     <PROGRAMNAME>SAPLID_TAXNUM</PROGRAMNAME>
     <ENHMODE>S</ENHMODE>
     <FULL_NAME>\FU:TAXNUMBER_CHECK_DUPL_VENDOR\SE:BEGIN\EI</FULL_NAME>
     <SOURCE>
      <item/>
      <item>* Verifica se o Fornecedor é passivel de criar mesmo com o CNPJ duplicado</item>
      <item>CHECK zcl_bupa_tax_number_check=&gt;check_bp_duplicity_check(</item>
      <item>*    iv_partner = N/A</item>
      <item>*    iv_taxnum  = vendor-stcd1</item>
      <item>is_vendor  = vendor                                     ) EQ zcl_bupa_tax_number_check=&gt;co_check_tax_id.</item>
      <item/>
     </SOURCE>
    </ENH_HOOK_IMPL>
   </ENHANCEMENTS>
   <SPACES>
    <item>
     <FULL_NAME>\FU:TAXNUMBER_CHECK_DUPL_VENDOR\SE:BEGIN\EI</FULL_NAME>
     <SPACES>
      <item>0</item>
      <item>0</item>
      <item>2</item>
      <item>0</item>
      <item>0</item>
      <item>4</item>
      <item>0</item>
     </SPACES>
    </item>
   </SPACES>
  </asx:values>
